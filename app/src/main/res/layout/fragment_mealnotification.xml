<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/header" />

    <com.custom.view.TextView200XLight
        android:id="@+id/SubHeading_UnitSettings"
        android:textSize="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="20dp"       
        android:text="Goals"
        android:textColor="@color/black"
        android:textStyle="normal" />
    
    <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="1dp"                               
                android:background="@color/light_grey"
                android:orientation="vertical" >
            </LinearLayout>

        <RelativeLayout
        android:id="@+id/UserFrame_ItemUser"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="15dp" >
        
          

        <com.custom.view.TextView300Light
            android:id="@+id/UserName_ItemUser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="15dp"
           
            android:clickable="false"
            android:text="Breakfast "
            android:textColor="@color/black"
            android:textSize="17sp"
            android:textStyle="normal" />

         <TextView
            android:id="@+id/textalarmtime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/toggle_btn_on"
            android:layout_toRightOf="@+id/UserName_ItemUser"
            android:layout_marginLeft="7dp"
            android:clickable="false"
            android:text=""
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="normal" />
        <ToggleButton
        android:id="@+id/toggle_btn_on"
        android:layout_width="45dp"
        android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:checked="false"
             android:background="@drawable/toggle_on_off_selector"
        android:text=""
        android:textOff=""
        android:textOn=""
            android:layout_marginRight="10dp"
        />
      <!--   <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
        
            android:background="@drawable/arrow"
            android:clickable="false"
            android:textSize="25sp" /> -->

    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="5dp"
        android:background="@color/light_grey"
        android:clickable="false"
        android:orientation="vertical" >

    </LinearLayout>
    
        <RelativeLayout
        android:id="@+id/UserFrame_ItemUser2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="15dp" >
        
          

        <com.custom.view.TextView300Light
            android:id="@+id/UserName_ItemUser2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="15dp"
           
            android:clickable="false"
            android:text="Lunch "
            android:textColor="@color/black"
            android:textSize="17sp"
            android:textStyle="normal" />

         <TextView
            android:id="@+id/textalarmtime2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/toggle_btn_on2"
            android:layout_toRightOf="@+id/UserName_ItemUser2"
            android:layout_marginLeft="7dp"
            android:clickable="false"
            android:text=""
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="normal" />
        <ToggleButton
        android:id="@+id/toggle_btn_on2"
        android:layout_width="45dp"
        android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:checked="false"
             android:background="@drawable/toggle_on_off_selector"
        android:text=""
        android:textOff=""
        android:textOn=""
            android:layout_marginRight="10dp"
        />
      <!--   <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
        
            android:background="@drawable/arrow"
            android:clickable="false"
            android:textSize="25sp" /> -->

    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="5dp"
        android:background="@color/light_grey"
        android:clickable="false"
        android:orientation="vertical" >

    </LinearLayout>
    
        <RelativeLayout
        android:id="@+id/UserFrame_ItemUser3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="15dp" >
        
          

        <com.custom.view.TextView300Light
            android:id="@+id/UserName_ItemUser3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="15dp"
           
            android:clickable="false"
            android:text="Dinner "
            android:textColor="@color/black"
            android:textSize="17sp"
            android:textStyle="normal" />

         <TextView
            android:id="@+id/textalarmtime3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/toggle_btn_on3"
            android:layout_toRightOf="@+id/UserName_ItemUser3"
            android:layout_marginLeft="7dp"
            android:clickable="false"
            android:text=""
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="normal" />
        <ToggleButton
        android:id="@+id/toggle_btn_on3"
        android:layout_width="45dp"
        android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:checked="false"
             android:background="@drawable/toggle_on_off_selector"
        android:text=""
        android:textOff=""
        android:textOn=""
            android:layout_marginRight="10dp"
        />
      <!--   <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
        
            android:background="@drawable/arrow"
            android:clickable="false"
            android:textSize="25sp" /> -->

    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="5dp"
        android:background="@color/light_grey"
        android:clickable="false"
        android:orientation="vertical" >

    </LinearLayout>
    

</LinearLayout>